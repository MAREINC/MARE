{{!< default}}
<div class="main">
	<div class="content">
		<div class="container">
			<div id="owl-demo" class="owl-carousel">
				{{#each slides}}
				<div class="item">
					<img class="lazyOwl" data-src="http://placehold.it/1100x400&text=featured-{{ @index }}" alt="Sample Image">
					<span class="featured-description hidden">
						<div class="featured-header">{{ heading }}</div>
						<div class="featured-tagline">{{ subHeading }}</div>
						<div class="featured-learn-more">{{ guideLabel }}</div>
					</span>
				</div>
				{{/each}}
			</div>
		</div>
		<div class="featured">
			<img src="http://placehold.it/400x250&text=featured" alt="featured-images-placeholder">
			<img src="http://placehold.it/400x250&text=featured" alt="featured-images-placeholder">
			<img src="http://placehold.it/400x250&text=featured" alt="featured-images-placeholder">
		</div>
	</div>
</div>