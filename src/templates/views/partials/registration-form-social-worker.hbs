<section class="social-worker-registration hidden">
	<form action="/register" class="registration-form form--social-worker-registration" method="POST">
		<input type="hidden" class="registration-type" name="registrationType" value="socialWorker">
		{{#if redirectUrl}}
			<input type="hidden" name="redirectUrl" value="{{ redirectUrl }}">
		{{/if}}

		<section class="social-worker-account-setup-section">
			<div class="form-section-header">Account Setup</div>

			<span class="required required-description">* Required</span>

			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="social-worker-first-name" class="input" name="firstName" data-parsley-error-message="required" required="">
					<label for="social-worker-first-name" class="label required-field">First Name</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="social-worker-last-name" class="input" name="lastName" data-parsley-error-message="required" required="">
					<label for="social-worker-last-name" class="label required-field">Last Name</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="email" id="social-worker-form-social-worker-email" class="input social-worker-email" name="email" data-parsley-type="email" data-parsley-trigger="change" data-parsley-error-message="valid email required" required="">
					<label for="social-worker-form-social-worker-email" class="label required-field">Email Address</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="password" id="social-worker-password" class="input" name="password"
							data-parsley-trigger="focusout"
							data-parsley-minlength="8" data-parsley-minlength-message="password must be 8 or more characters"
							data-parsley-required-message="password is required" required=""
							data-parsley-pattern="/(?=.*\d)(?=.*[a-z])(?=.*[A-Z])/" data-parsley-pattern-message="password does not meet requirements">
					<label for="social-worker-password" class="label required-field">Password</label>
				</div>
				<div class="col-sm-12">
					<input type="password" id="social-worker-confirm-password" class="input" name="confirmPassword"
							data-parsley-trigger="focusout"
							data-parsley-minlength="8" data-parsley-minlength-message="password must be 8 or more characters"
							data-parsley-required-message="password is required" required=""
							data-parsley-pattern="/(?=.*\d)(?=.*[a-z])(?=.*[A-Z])/" data-parsley-pattern-message="password does not meet requirements"
							data-parsley-equalto="#social-worker-password" data-parsley-equalto-message="passwords must match">
					<label for="social-worker-confirm-password" class="label required-field">Confirm Password</label>
				</div>
			</div>
			<div>
				<p class="form-field-note">Note: passwords must be at least 8 characters long, contain at least one number, one uppercase letter, and one lowercase letter.</p>
			</div>
		</section>

		<section class="social-worker-contact-details-section">
			<div class="form-section-header">Contact Details</div>
			<div class="row">
				<div class="col-sm-24 form-group">
					<span class="required-field">Positions (check all that apply)</span>
				</div>
			</div>
			<div id="positions-error-container" class="row form-group error-container">
				{{#each socialWorkerPositions}}
				<div class="col-sm-12 form-group">
					<label class="checkbox-inline">
						<input type="checkbox" name="positions" value="{{ _id }}" data-parsley-errors-messages-disabled="true" data-parsley-class-handler="#positions-error-container" required="">{{ position }}
					</label>
				</div>
				{{/each}}
			</div>
			<div class="row">
				<div class="col-sm-12 form-group social-worker-title-group">
					<input type="text" id="social-worker-title" class="input social-worker-title" name="socialWorkerTitle" required="" data-parsley-error-message="required">
					<label for="social-worker-title" class="label required-field">Title</label>
				</div>
				<div class="col-sm-12 form-group">
					<input type="text" id="social-worker-form-social-worker-agency" class="input social-worker-agency" name="agency" data-parsley-error-message="required" required="">
					<label for="social-worker-form-social-worker-agency" class="label required-field">Agency</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="social-worker-work-phone" class="input" name="workPhone" data-parsley-required-message="required" required=""
						data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format" >
					<label for="social-worker-work-phone" class="label required-field">Work Phone</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="social-worker-mobile-phone" class="input" name="mobilePhone" data-parsley-pattern="/^\d{3}-\d{3}-\d{4}$/" data-parsley-pattern-message="Please use xxx-xxx-xxxx format">
					<label for="social-worker-mobile-phone" class="label">Mobile Phone</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<select id="social-worker-preferred-phone" class="input social-worker-preferred-phone-select" name="preferredPhone" data-parsley-errors-messages-disabled="true" required="">
						<option value=""></option>
						<option value="work">work</option>
						<option value="mobile">mobile</option>
					</select>
					<label for="social-worker-preferred-phone" class="label required-field">Preferred Phone</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<input type="text" id="social-worker-street-1" class="input" name="street1" data-parsley-error-message="required" required="">
					<label for="social-worker-street-1" class="label required-field">Street 1</label>
				</div>
				<div class="col-sm-12">
					<input type="text" id="social-worker-street-2" class="input" name="street2">
					<label for="social-worker-street-2" class="label">Street 2</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12 form-group city-container">
					<select class="input city" name="city" data-parsley-trigger="change" data-parsley-errors-messages-disabled="true" required="">
						<option value=""></option>
						{{#each citiesAndTowns}}
						<option value="{{ _id }}">{{ cityOrTown }}</option>
						{{/each}}
					</select>
					<label for="city" class="label required-field">City</label>
				</div>
				<div class="col-sm-12 form-group non-ma-city-container hidden">
					<input type="text" id="social-worker-non-ma-city" class="input non-ma-city" name="nonMACity" data-parsley-error-message="required" data-parsley-required="false">
					<label for="social-worker-non-ma-city" class="label required-field">City</label>
				</div>
				<div class="col-sm-12 form-group">
					<label class="checkbox-inline">
						<input type="checkbox" class="is-not-ma-city-checkbox" name="isNotMACity">I live outside MA
					</label>
				</div>
			</div>
			<div class="row form-group">
				<div class="col-sm-12">
					<select id="social-worker-state" class="input state-select" name="state">
					{{#each states}}
						{{#if defaultSelection}}
							<option value="{{_id}}" selected="selected">{{state}}</option>
						{{else}}
							<option value="{{_id}}">{{state}}</option>
						{{/if}}
					{{/each}}
					</select>
					<label for="social-worker-state" class="label required-field">State</label>
				</div>
				<div class="col-sm-12">
					<input type="number" id="social-worker-zip-code" class="input" name="zipCode" data-parsley-length="[5, 5]" data-parsley-required-message="required" required=""
						data-parsley-pattern="/^[0-9]{5}$/" data-parsley-pattern-message="Please use xxxxx format">
					<label for="social-worker-zip-code" class="label required-field">Zip Code</label>
				</div>
			</div>
		</section>

		<section class="social-worker-mare-and-you-section">
			<div class="form-section-header">Mare &amp; You</div>
			<div class="row">
				<div class="col-sm-24 form-group">
					<label class="label marg-bottom-xs-5">Which communications would you like to receive?</label>
				</div>
				<div class="col-sm-16">
					{{#each mailingLists}}
					<div class="row form-group">
						<div class="col-sm-12">
							<label class="label marg-bottom-xs-5">{{ displayText }}</label>
						</div>
						<div class="col-sm-12">
							<label class="radio-inline"><input type="radio" name="mailingLists[{{@index}}]" value={{ _id }}
									checked="checked">Yes</label>
							<label class="radio-inline"><input type="radio" name="mailingLists[{{@index}}]" value="">No</label>
						</div>
					</div>
					{{/each}}
				</div>
			</div>
		</section>

		<button class="button button--teal register" type="submit">Register</button>
	</form>
</section>
